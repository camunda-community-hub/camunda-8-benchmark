  <decision id="decision-classify-event" name="Event Classification">
    <decisionTable id="DecisionTable_0kfhl59">
      <input id="Input_1" label="Metric Value">
        <inputExpression id="InputExpression_1" typeRef="string">
          <text>metric_value</text>
        </inputExpression>
      </input>
      <input id="InputClause_0w48qwj" label="Severity">
        <inputExpression id="LiteralExpression_1yw0zto" typeRef="number">
          <text>number(severity_ordinal)</text>
        </inputExpression>
      </input>
      <output id="Output_1" label="Event Type Classification" name="eventType" typeRef="string">
        <outputValues id="UnaryTests_0jrl471">
          <text>"High priority","Low priority","Further Verification","Ignore"</text>
        </outputValues>
      </output>
      <rule id="DecisionRule_1304bl5">
        <inputEntry id="UnaryTests_0ae5lmz">
          <text>"High priority"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0u4i8fo">
          <text>&gt; 90</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1a8exyi">
          <text>"High priority"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0m9thg1">
        <inputEntry id="UnaryTests_1qzy04m">
          <text>"High priority"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1gazzxa">
          <text>&lt;=90</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_011m47v">
          <text>"Low priority"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0twex84">
        <inputEntry id="UnaryTests_1a1eybe">
          <text>"Low priority"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0ngpqs2">
          <text>&gt;25</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_06cuj5v">
          <text>"High priority"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1noomxr">
        <inputEntry id="UnaryTests_1yqshck">
          <text>"Low priority"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1vv424g">
          <text>&lt;=25</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1tje339">
          <text>"Low priority"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_16cpqrp">
        <inputEntry id="UnaryTests_0nhx72d">
          <text>"Ignore"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0ctdilw">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1v82gdx">
          <text>"Ignore"</text>
        </outputEntry>
      </rule>
